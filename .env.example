# Copy this file to .env and fill in your actual values
# DO NOT commit .env files to git!

# OpenCage Geocoding API Key (required)
# Get from: https://opencagedata.com/
OPENCAGE_API_KEY=your_opencage_api_key_here

# OpenAI API Key (optional - for enhanced AI descriptions)
# Get from: https://platform.openai.com/
OPENAI_API_KEY=your_openai_api_key_here

# Database Configuration (for local development)
# Leave these empty to use H2 in-memory database
DATABASE_URL=
DATABASE_USERNAME=
DATABASE_PASSWORD=
DATABASE_DRIVER=

# JawsDB URL (for production - set in Heroku)
JAWSDB_URL=